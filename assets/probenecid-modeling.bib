@article{wattanagoon2009,
  title = {Pharmacokinetics of {{High-Dose Oseltamivir}} in {{Healthy Volunteers}}},
  author = {Wattanagoon, Y. and Stepniewska, K. and Lindeg{\aa}rdh, N. and Pukrittayakamee, S. and Silachamroon, U. and Piyaphanee, W. and Singtoroj, T. and Hanpithakpong, W. and Davies, G. and Tarning, J. and Pongtavornpinyo, W. and Fukuda, C. and Singhasivanon, P. and Day, N. P. J. and White, N. J.},
  year = {2009},
