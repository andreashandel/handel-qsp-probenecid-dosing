  author = {Vree, T. B. and {Van Ewijk-Beneken Kolmer}, E. W. J. and Wuis, E. W. and Hekster, Y. A.},
  year = {1992},
  month = oct,
  journal = {Pharmaceutisch Weekblad},
  volume = {14},
