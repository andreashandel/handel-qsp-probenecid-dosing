  pii = {AAC.00069-15},
  pmc = {PMC4538552},
  pmid = {26100715},
  pubmodel = {Print-Electronic},
  pubstatus = {ppublish},
  revised = {2018-11-13},
  keywords = {antagonists \& inhibitors,Antiviral Agents,Biological,drug effects,enzymology,metabolism,Models,Neuraminidase,Orthomyxoviridae,Oseltamivir,pharmacology},
