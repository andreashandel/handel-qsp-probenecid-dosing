@article{utturkar00,
  title = {Development of {{Physiologically Based Pharmacokinetic Model}} ({{PBPK}}) of {{BMP2}} in {{Mice}}},
  author = {Utturkar, Aditya and Paul, Bikram and Akkiraju, Hemanth and Bonor, Jeremy and Dhurjati, Prasad and Nohe, Anja},
  year = {0},
  journal = {Biological Systems: Open Access},
